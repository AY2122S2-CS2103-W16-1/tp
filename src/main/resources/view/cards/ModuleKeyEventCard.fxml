<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
        </columnConstraints>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10" percentWidth="20"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10" percentWidth="30"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10" percentWidth="50"/>
            </columnConstraints>
            <Label fx:id="id" styleClass="module-key-event-index" GridPane.columnIndex="0" />
            <Label  fx:id="moduleKeyEventType" styleClass="module-key-event-type" GridPane.columnIndex="1"/>
            <Label fx:id="moduleKeyEventTime" styleClass="module-key-event-time" GridPane.columnIndex="2"/>
        </GridPane>
        <rowConstraints>
            <RowConstraints />
        </rowConstraints>
    </GridPane>
</HBox>

